{
  "name": "moj-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "gcp-predeploy": "find ./build -name '\\[...*' -exec bash -c 'mv \"$1\" \"${1/.../@@@}\"' -- {} \\;",
    "gcp-build": "find ./build -name '\\[@@@*' -exec bash -c 'mv \"$1\" \"${1/@@@/...}\"' -- {} \\;",
    "deploy": "npm run build && npm run gcp-predeploy && gcloud app deploy",
    "build": "rm -rf ./build && NODE_ENV=production next build",
    "testing-build": "next build",
    "dev": "next dev",
    "start": "next start"
  },
  "dependencies": {
    "@ant-design/charts": "^1.2.14",
    "@ant-design/icons": "^4.5.0",
    "@google-cloud/storage": "^5.11.0",
    "ajv": "^8.6.3",
    "antd": "^4.12.3",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "js-cookie": "^3.0.0",
    "mongoose": "^5.12.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.30",
    "next": "^11.1.2",
    "next-auth": "^3.23.1",
    "next-connect": "^0.10.1",
    "next-pwa": "^5.2.24",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-pdf": "^5.3.2",
    "react-sizeme": "^3.0.2",
    "swr": "^0.5.6"
  }
}
